    "sisdai-mapas": "git+https://codigo.conahcyt.mx/sisdai/sisdai-mapas#v1.5.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.4.0",
